<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- Defines the sync interval in minutes. Local changes are always synced immediately. -->
    <add key="AutoSyncInterval" value="10" />
    <!-- Defines whether to show balloon notifications (True) or not (False) -->
    <add key="DisplayNotifications" value="False" />
    <add key="EnableDebugLog" value="False" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
</configuration>